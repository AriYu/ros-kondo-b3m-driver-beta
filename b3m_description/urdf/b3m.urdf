<?xml version="1.0"?>
<robot name="origins">
  <link name="base_link">
    <visual>
      <geometry>
		<box size="0.3 0.1 0.1" />
      </geometry>
	  <origin rpy="0 0 0" xyz="0.0 0.0 0.0" />
    </visual>
  </link>

  <link name="wheel0">
    <visual>
      <geometry>
		<cylinder radius="0.5" length="0.05" />
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="base_to_wheel0" type="continuous">
	<axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel0"/>
    <origin xyz="0.3 0.1 0.1"/>
  </joint>

  <link name="wheel1">
    <visual>
      <geometry>
		<cylinder radius="0.5" length="0.05" />
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0"/>
    </visual>
  </link>
  
  <joint name="base_to_wheel1" type="continuous">
	<axis xyz="0 1 0"/>
    <parent link="base_link"/>
    <child link="wheel1"/>
    <origin xyz="0.6 0.1 0.1"/>
  </joint>

</robot>
